# Specifies the location of the ports software directory.
# Example: /Users/john/Documents/darwinports/software
#sysportpath		/usr/ports/

# Print debug messages - You probably want 'yes' if
# you're maintaining ports
ports_debug		no

# Print informational messages - You probably want
# 'yes' if you're maintaining ports
ports_verbose		no

# Set the directory in which to install ports
prefix			/opt/local

# Where the "ports registry" lives.
registry.path		/opt/local/var/db/receipts

